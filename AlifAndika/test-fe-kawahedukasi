function hasNumber(input){
return /\d/.test(input);
}
function showingNumber(input){
var data = input.match(/\d+/g).map(Number);
var mappingNumber = data.map((i) => i)
return `Sistem kami menolak untuk inputan berisi angka 167283 ${mappingNumber}`
}
